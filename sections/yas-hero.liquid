<script>
  document.addEventListener("DOMContentLoaded", function () {
    let swiper = new Swiper("#shopify-section-{{section.id}} .swiper-container", {
      loop: true,
      spaceBetween: 30,
      effect: "fade",
      fadeEffect: {
        crossFade: true
      },
      navigation: {
        nextEl: "#shopify-section-{{section.id}} .swiper-b-prev",
        prevEl: "#shopify-section-{{section.id}} .swiper-b-next",
      },
      pagination: {
        el: "#shopify-section-{{section.id}} .swiper-pagination",
        clickable: true,
      },
    });


  
    
  });
</script>
{% schema %}
{
  "name": "Hero Banner",
  "tag": "section",
  "class": "section hero_banner",
  "settings": [
    {
      "type": "header",
      "content": "General Settings"
    },
     { "type": "color", 
       "id": "background_color", 
       "label": "Background color", 
       "default": "transparent" 
     },
     {
      "type": "select",
      "id": "font_variant",
      "label": "Font Family",
      "options": [
        { "value": "default", "label": "Default by theme" },
        { "value": "custom", "label": "Own choice" }
      ],
      "default": "default"
    },
    {
      "type": "font_picker",
      "label": "Regular Font",
      "id": "title_font",
      "default": "montserrat_n4"
    },
  
    
    {
      "type": "select",
      "id": "section_width",
      "label": "Section Width",
      "options": [
        { "value": "full", "label": "Full Width" },
        { "value": "contained", "label": "Contained" }
      ],
      "default": "contained"
    },
      { "type": "select", "id": "size", "label": "Banner Height", "options": [{ "value": "small", "label": "Small" }, { "value": "medium", "label": "Medium" }, { "value": "big", "label": "Big" }], "default": "big" },
   
    {
      "type": "range",
      "id": "padding_x_desk",
      "label": "Horizontal Padding",
      "info": "for desktop",
      "min": 0,
      "max": 200,
      "step": 5,
      "default": 120
    },
       {
      "type": "range",
      "id": "padding_y_desk",
      "label": "Vertical Padding",
      "info": "for desktop",   
      "min": 0,
      "max": 100,
            "step": 5,
      "default": 75
    },
    {
      "type": "range",
      "id": "padding_x_mob",
      "label": "Horizontal Padding",
      "info": "for mobile",
      "min": 0,
      "max": 100,
      "step": 5,
      "default": 20
    },
    {
      "type": "range",
      "id": "padding_y_mob",
      "label": "Vertical Padding",
      "info": "for mobile",   
      "min": 0,
      "max": 100,
