<div class="content-wrapper-{{ forloop.index }}">
   
   <div class="wrap_image">
      {% if block.settings.image %}
 

        <picture>
            {% if block.settings.imagem %}
          <source srcset="{{ block.settings.imagem | img_url: 'x700' }}" media="(max-width: 768px)">
          {% endif %}
          <img src="{{ block.settings.image | img_url: 'x1200' }}" alt="Section Image"/>
        </picture>
       
      {% endif %}
    </div>
    <div class="image-with-text-content">
      {%- if block.settings.title != blank or block.settings.under_title_text != blank -%}
      <div class="heading-{{ forloop.index }}">
        {% if block.settings.uppertitle != blank %}
      <div class="uppertitle-{{ forloop.index }}">{{ block.settings.uppertitle }} {% for i in (1..5) %}{% render 'star_comparison' %}{% endfor %}</div>
    {% endif %}
        <div class="title-{{ forloop.index }}">
          {%- if block.settings.title != blank -%}
            {{ block.settings.title }}
          {%- endif -%}
        </div>
        <div class="subtitle-{{ forloop.index }}">
          {%- if block.settings.subtitle != blank -%}
            {{ block.settings.subtitle }}
          {%- endif -%}
        </div>
        
      </div>
    {%- endif -%}
      <ul class="image-with-text-list-{{ forloop.index }}">
        <li>
          {% if block.settings.icon_1 != blank %}
            <img src="{{ block.settings.icon_1 | img_url: '24x24' }}" alt="Icon 1">
          {% else %}
            {% if block.settings.icon_1_text != blank %}
          <span class="material-symbols-outlined">     {{ block.settings.icon_1_text }}</span>
            {% else %}
              {% render 'icon_number_1' %}
            {% endif %}
          {% endif %}
          {{ block.settings.point_1_text }}
        </li>
        <li>
          {% if block.settings.icon_2 != blank %}
            <img src="{{ block.settings.icon_2 | img_url: '24x24' }}" alt="Icon 2">
          {% else %}
            {% if block.settings.icon_2_text != blank %}
              <span class="material-symbols-outlined"> {{ block.settings.icon_2_text }}</span>
            {% else %}
              {% render 'icon_number_2' %}
            {% endif %}
          {% endif %}
          {{ block.settings.point_2_text }}
        </li>
        <li>
          {% if block.settings.icon_3 != blank %}
            <img src="{{ block.settings.icon_3 | img_url: '24x24' }}" alt="Icon 3">
          {% else %}
            {% if block.settings.icon_3_text != blank %}
              <span class="material-symbols-outlined">{{ block.settings.icon_3_text }}</span>
            {% else %}
              {% render 'icon_number_3' %}
            {% endif %}
          {% endif %}
          {{ block.settings.point_3_text }}
        </li>
      </ul>
  
       <a href="{{ block.settings.button_link }}" class="button-{{ forloop.index }}">{{ block.settings.button_text }}</a>
    
