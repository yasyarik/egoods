          <img src="{{ section.settings.picon2 |img_url:'x72' }}" class="payicon"/>
      {% endif %}
         {% if section.settings.picon3 %}
          <img src="{{ section.settings.picon3 |img_url:'x72' }}" class="payicon"/>
      {% endif %}
         {% if section.settings.picon4 %}
          <img src="{{ section.settings.picon4 |img_url:'x72' }}" class="payicon"/>
      {% endif %}
         {% if section.settings.picon5 %}
          <img src="{{ section.settings.picon5 |img_url:'x72' }}" class="payicon"/>
      {% endif %}
