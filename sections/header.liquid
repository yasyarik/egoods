{% if section.settings.logo_position == "left" %}
<div class="header__main py-10 py-lg-0"> 
  <div class="header__container container">
      <div class="pure-g align-items-center justify-content-between">
          <div class="header__toggle js-header-toggle d-block d-lg-none"><!-- header toggle -->
              <span></span>
              <span></span>
              <span></span>
          </div><!-- ./header toggle -->
          <div class="header__left pure-u-lg-14-24">
              <div class="header__nav header__nav--desktop"> <!-- header nav for desktop -->
                  <div class="header__logo"><!-- header logo -->
                      <a href="{{ routes.root_url }}" aria-label="Rocket">
                          {%- if section.settings.logo != blank -%}
                            <img src="{{ section.settings.logo | img_url: 'master' }}" class="pure-img" alt="{{ shop.name }}" width="{{ section.settings.logo_width }}" height="{{ section.settings.logo_height }}" />
                          {% else %}
                            <span class="h2">{{ shop.name }}</span>
                          {% endif %}
                      </a>
                  </div><!-- ./header logo -->
                  {% if section.settings.menu != blank %}
                    <div class="header__menu"><!-- header menu -->
                        <ul class="header__list">
                          {%- for link in section.settings.menu.links -%}
                            {%- assign hasMegaMenu = false -%}
                            {% if section.blocks.size > 0 %}
                              {% for block in section.blocks %}
                                {% if block.type == "mega-menu" %}
                                  {% if block.settings.title != blank %}
                                    {% if block.settings.title == link.title %}
                                      <li class="header__has-submenu {% if link.current %}menu-item--active{% endif %}">
                                        <a href="{{ link.url }}">{{ link.title | escape }} <span class="material-icons">keyboard_arrow_down</span></a>
                                        <div class="header__mega-menu custom-ul-class">
                                            <div class="pure-g">
                                                {% if block.settings.sub_nav_1 != blank or block.settings.sub_nav_2 != blank %}
                                                  <div class="pure-u-7-24">
                                                    {% if block.settings.navigation_block_title != blank %}
                                                      <h4 class="header__mm-title">{{block.settings.navigation_block_title}}</h4>
                                                    {% endif %}
                                                      <div class="pure-g">
                                                        {% if block.settings.sub_nav_1 != blank %}
                                                          <div class="pure-u-1-2">
                                                            {% if block.settings.sub_nav_1_title != blank %}
                                                              <h5 class="header__mm-list-title">{{block.settings.sub_nav_1_title}}</h5>
                                                            {% endif %}
                                                              <ul class="list-unstyled">
                                                                {%- for subNav1Link in block.settings.sub_nav_1.links -%}
                                                                  <li><a href="{{subNav1Link.url}}">{{subNav1Link.title | escape}}</a></li>
                                                                {%- endfor -%}
                                                              </ul>
                                                          </div>
                                                        {% endif %}
                                                        {% if block.settings.sub_nav_2 != blank %}
                                                          <div class="pure-u-1-2">
                                                            {% if block.settings.sub_nav_2_title != blank %}
                                                              <h5 class="header__mm-list-title">{{block.settings.sub_nav_2_title}}</h5>
                                                            {% endif %}
                                                              <ul class="list-unstyled">
                                                                {%- for subNav2Link in block.settings.sub_nav_2.links -%}
                                                                  <li><a href="{{subNav2Link.url}}">{{subNav2Link.title | escape}}</a></li>
                                                                {%- endfor -%}
                                                              </ul>
                                                          </div>
                                                        {% endif %}
                                                      </div>
                                                  </div>
                                                {% else %}
                                                  <div class="pure-u-7-24">
                                                      <h4 class="header__mm-title">Shop Websitename</h4>
                                                      <div class="pure-g">
                                                          <div class="pure-u-1-2">
                                                              <h5 class="header__mm-list-title">Category</h5>
                                                              <ul class="list-unstyled">
                                                                  <li><a href="#">Inner category</a></li>
                                                                  <li><a href="#">Inner category</a></li>
                                                                  <li><a href="#">Inner category</a></li>
                                                                  <li><a href="#">Inner category</a></li>
                                                                  <li><a href="#">Inner category</a></li>
                                                              </ul>
                                                          </div>
                                                          <div class="pure-u-1-2">
                                                              <h5 class="header__mm-list-title">Category</h5>
                                                              <ul class="list-unstyled">
                                                                  <li><a href="#">Inner category</a></li>
                                                                  <li><a href="#">Inner category</a></li>
                                                                  <li><a href="#">Inner category</a></li>
                                                                  <li><a href="#">Inner category</a></li>
                                                                  <li><a href="#">Inner category</a></li>
                                                              </ul>
                                                          </div>
                                                      </div>
                                                  </div>
                                                {% endif %}
                                                {% if block.settings.product_1 != blank or block.settings.product_2 != blank %}
                                                <div class="pure-u-7-24">
                                                    {% if block.settings.product_block_title != blank %}
                                                      <h4 class="header__mm-title">{{block.settings.product_block_title}}</h4>
                                                    {% endif %}
                                                    <div class="product__grid">
                                                      {% if block.settings.product_1 != blank %}
                                                        {% assign megaMenuProduct1 = all_products[block.settings.product_1] %}
                                                        {%- assign megaMenuProduct1CurrentVariant = megaMenuProduct1.selected_or_first_available_variant -%}
                                                        <div class="product product--style-2">
                                                            <div class="product__image-container">
                                                                <a href="{{megaMenuProduct1.url}}" class="product__image ratio1by1">
                                                                    {% if megaMenuProduct1.featured_image.src != blank %}
                                                                      <img src="{{ megaMenuProduct1.featured_image.src | img_url: '140x' }}" class=" pure-img" alt="" loading="lazy"/>
                                                                    {% else %}
                                                                      <img data-src="{{ 'bg-gray-img.jpg' | asset_url }}" class="lazyload pure-img" alt="" />
                                                                    {% endif %}
                                                                </a>
                                                            </div>
                                                            <div class="product__detail">
                                                                {%- if block.settings.show_product_rating -%}
                                                                  <div class="rating">
                                                                      <span class="shopify-product-reviews-badge" data-id="{{ megaMenuProduct1.id }}"></span>
                                                                  </div>
                                                                {% endif %}
                                                                <a href="{{megaMenuProduct1.url}}" class="product__title">{{megaMenuProduct1.title}}</a>
                                                                <div class="product__meta">
                                                                    <div class="product__price">
                                                                      {%- if megaMenuProduct1CurrentVariant.compare_at_price > megaMenuProduct1CurrentVariant.price -%}
                                                                        <span class="product__price-sale">{{ megaMenuProduct1CurrentVariant.compare_at_price | money}}</span>
                                                                      {%- endif -%}
                                                                        <span class="product__price-original">{{ megaMenuProduct1CurrentVariant.price | money}}</span>
                                                                    </div>
                                                                    {%- if megaMenuProduct1CurrentVariant.compare_at_price > megaMenuProduct1CurrentVariant.price -%}
                                                                    {% assign megaMenuProduct1Save =  megaMenuProduct1CurrentVariant.compare_at_price | minus: megaMenuProduct1CurrentVariant.price | times: 100.0 | divided_by: megaMenuProduct1CurrentVariant.compare_at_price | money_without_currency | times: 100 | remove: '.0' | plus: 0 %}
                                                                      {% if megaMenuProduct1Save > 0 %}
                                                                        <span class="badge" {% if section.settings.save_badge_color != blank %}style="background-color: {{ section.settings.save_badge_color }};"{% endif %}>Save {{megaMenuProduct1Save}}%</span>
                                                                      {% endif %}
                                                                    {%- endif -%}
                                                                </div>
                                                            </div>
                                                        </div>
                                                      {% endif %}
                                                      {% if block.settings.product_2 != blank %}
                                                        {% assign megaMenuProduct2 = all_products[block.settings.product_2] %}
                                                        {%- assign megaMenuProduct2CurrentVariant = megaMenuProduct2.selected_or_first_available_variant -%}
                                                        <div class="product product--style-2">
                                                            <div class="product__image-container">
                                                                <a href="{{megaMenuProduct2.url}}" class="product__image ratio1by1">
                                                                    {% if megaMenuProduct2.featured_image.src != blank %}
                                                                      <img src="{{ megaMenuProduct2.featured_image.src | img_url: '140x' }}" class=" pure-img" alt="" loading="lazy" />
                                                                    {% else %}
                                                                      <img data-src="{{ 'bg-gray-img.jpg' | asset_url }}" class="lazyload pure-img" alt="" />
                                                                    {% endif %}
                                                                </a>
                                                            </div>
                                                            <div class="product__detail">
                                                                {%- if block.settings.show_product_rating -%}
                                                                  <div class="rating">
                                                                      <span class="shopify-product-reviews-badge" data-id="{{ megaMenuProduct2.id }}"></span>
                                                                  </div>
                                                                {% endif %}
                                                                <a href="{{megaMenuProduct2.url}}" class="product__title">{{megaMenuProduct2.title}}</a>
                                                                <div class="product__meta">
                                                                    <div class="product__price">
                                                                      {%- if megaMenuProduct2CurrentVariant.compare_at_price > megaMenuProduct2CurrentVariant.price -%}
                                                                        <span class="product__price-sale">{{ megaMenuProduct2CurrentVariant.compare_at_price | money}}</span>
                                                                      {%- endif -%}
                                                                        <span class="product__price-original">{{ megaMenuProduct2CurrentVariant.price | money}}</span>
                                                                    </div>
                                                                    {%- if megaMenuProduct2CurrentVariant.compare_at_price > megaMenuProduct2CurrentVariant.price -%}
                                                                    {% assign megaMenuProduct2Save =  megaMenuProduct2CurrentVariant.compare_at_price | minus: megaMenuProduct2CurrentVariant.price | times: 100.0 | divided_by: megaMenuProduct2CurrentVariant.compare_at_price | money_without_currency | times: 100 | remove: '.0' | plus: 0 %}
                                                                      {% if megaMenuProduct2Save > 0 %}
                                                                        <span class="badge" {% if section.settings.save_badge_color != blank %}style="background-color: {{ section.settings.save_badge_color }};"{% endif %}>Save {{megaMenuProduct2Save}}%</span>
                                                                      {% endif %}
                                                                    {%- endif -%}
                                                                </div>
                                                            </div>
                                                        </div>
                                                      {% endif %}
                                                    </div>
                                                </div>
                                                {% else %}
                                                  <div class="pure-u-7-24">
                                                      <h4 class="header__mm-title">Featured Products</h4>
                                                      <div class="product__grid">
                                                          <div class="product product--style-2">
                                                              <div class="product__image-container">
                                                                  <a href="#" class="product__image ratio1by1">
                                                                      {{ 'product-1' | placeholder_svg_tag: 'lazyload pure-img svg__thumb' }}
                                                                  </a>
                                                              </div>
                                                              <div class="product__detail">
                                                                  <div class="rating">
                                                                      <span class="rating__star">
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                      </span>
                                                                      <span class="rating__text">83 <span class="d-none d-md-inline">Reviews</span></span>
                                                                  </div>
                                                                  <a href="#" class="product__title">Product name goes here</a>
                                                                  <div class="product__meta">
                                                                      <div class="product__price">
                                                                          <span class="product__price-sale">£19.99</span>
                                                                          <span class="product__price-original">£9.99</span>
                                                                      </div>
                                                                      <span class="badge">Save 50%</span>
                                                                  </div>
                                                              </div>
                                                          </div>
                                                          <div class="product product--style-2">
                                                              <div class="product__image-container">
                                                                  <a href="#" class="product__image ratio1by1">
                                                                      {{ 'product-2' | placeholder_svg_tag: 'lazyload pure-img svg__thumb' }}
                                                                  </a>
                                                              </div>
                                                              <div class="product__detail">
                                                                  <div class="rating">
                                                                      <span class="rating__star">
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                          <svg class="active" width="13" height="13">
                                                                          <use xlink:href="#ic-star"></use>
                                                                          </svg>
                                                                      </span>
                                                                      <span class="rating__text">83 <span class="d-none d-md-inline">Reviews</span></span>
                                                                  </div>
                                                                  <a href="#" class="product__title">Product name goes here</a>
                                                                  <div class="product__meta">
                                                                      <div class="product__price">
                                                                          <span class="product__price-sale">£19.99</span>
                                                                          <span class="product__price-original">£9.99</span>
                                                                      </div>
                                                                      <span class="badge">Save 50%</span>
                                                                  </div>
                                                              </div>
                                                          </div>
                                                      </div>
                                                  </div>
                                                {% endif %}
                                                {% if block.settings.category_image != blank and block.settings.category_title != blank %}
                                                  <div class="pure-u-10-24">
                                                      <div class="portfolio portfolio--style-2">
                                                          <figure class="portfolio__item ratio21by5">
                                                              <img data-src="{{ block.settings.category_image | img_url: 'master' }}" class="img-fluid lazyload" alt="" />
                                                              <figcaption class="portfolio__info">
                                                                  <h3 class="text-8 fw-400" {% if block.settings.category_title_color != blank %}style="color: {{ block.settings.category_title_color }}"{% endif %}>
                                                                    {% if block.settings.category_link != blank %}
                                                                      <a href="{{block.settings.category_link}}" {% if block.settings.category_title_color != blank %}style="color: {{ block.settings.category_title_color }}"{% endif %}></a>
                                                                    {% endif %}
                                                                      {{block.settings.category_title}}
                                                                    {% if block.settings.category_link != blank %}
                                                                      </a>
                                                                    {% endif %}
                                                                    </h3>
                                                                  {% if block.settings.category_link != blank %}
                                                                    <a href="{{block.settings.category_link}}" class="btn btn--icon-hand">{{block.settings.category_button_text}}</a>
                                                                  {% endif %}
                                                              </figcaption>
                                                          </figure>
                                                      </div>
                                                  </div>
                                                {% else %}
                                                  <div class="pure-u-10-24">
                                                      <div class="portfolio portfolio--style-2">
                                                          <figure class="portfolio__item ratio21by5">
                                                              {{ 'lifestyle-2' | placeholder_svg_tag: 'lazyload img-fluid svg__thumb' }}
                                                              <figcaption class="portfolio__info">
                                                                  <h3 class="text-8 fw-400"><a href="#">Category name</a></h3>
                                                                  <a href="#" class="btn btn--icon-hand">Shop Now</a>
                                                              </figcaption>
                                                          </figure>
                                                      </div>
                                                  </div>
                                                {% endif %}
                                            </div>
                                        </div>
                                      </li>
                                      {%- assign hasMegaMenu = true -%}
                                    {% break %}
                                    {% endif %}
                                  {% endif %}
                                {% endif %}
                              {% endfor %}
                            {% endif %}
                            {% if hasMegaMenu %}
                            {% else %}
                              <li class="{% if link.links != blank %}has-childrens{% endif %} {% if link.current %}menu-item--active{% endif %}">
                                <a href="{{ link.url }}">{{ link.title | escape }} {% if link.links != blank %}<span class="material-icons">keyboard_arrow_down</span>{% endif %}</a>
                                {% assign mainLinkTitle = link.title | escape %}
                                {% assign mainLinkLink = link.url %}
                                {%- if link.links != blank -%}
                                  <ul class="child-lists list-menu custom-ul-class" role="list" tabindex="-1">
                                    <li class="sub-menu-title-parent"><a href="{{ mainLinkLink }}">{{ mainLinkTitle }}</a></li>
                                    {%- for childlink in link.links -%}
                                      <li class="{% if childlink.links != blank %}has-childrens{% endif %} {% if childlink.current %}menu-item--active{% endif %}">
                                          <a href="{{ childlink.url }}">{{ childlink.title | escape }} {% if childlink.links != blank %}<span class="material-icons">keyboard_arrow_right</span>{% endif %}</a>
                                          {%- if childlink.links != blank -%}
                                            {% assign mainSubLinkTitle = childlink.title | escape %}
                                            {% assign mainSubLinkLink = childlink.url %}
                                            <ul class="sub-child-lists list-menu custom-ul-class" role="list" tabindex="-1">
                                              <li class="sub-menu-title"><span class="js-back-to-parent"><span class="material-icons">keyboard_arrow_left</span>{{ mainLinkTitle }}</span></li>
                                              <li class="sub-menu-title-parent"><a href="{{ mainSubLinkLink }}">{{ mainSubLinkTitle }}</a></li>
                                              {%- for grandchildlink in childlink.links -%}
                                                <li class="{% if grandchildlink.current %}menu-item--active{% endif %}">
                                                  <a href="{{ grandchildlink.url }}">{{ grandchildlink.title | escape }}</a>
                                                </li>
                                              {%- endfor -%}
                                            </ul>
                                          {%- endif -%}
                                      </li>
                                    {%- endfor -%}
                                  </ul>
                                {%- endif -%}
                              </li>
                            {% endif %}
                          {%- endfor -%}
                        </ul>
                    </div><!-- ./header menu -->
                  {% endif %}
              </div><!-- header nav for desktop END-->

              <div class="header__nav header__nav--mobile"><!-- header nav for Mobile -->
                  <div class="header__logo"><!-- header logo -->
                      <a href="{{ routes.root_url }}" aria-label="Rocket">
                          {%- if section.settings.logo != blank -%}
                            <img src="{{ section.settings.logo | img_url: 'master' }}" class="pure-img" alt="{{ shop.name }}" width="{{ section.settings.logo_width }}" height="{{ section.settings.logo_height }}" />
                          {% else %}
                            <span class="h2">{{ shop.name }}</span>
                          {% endif %}
                      </a>
                  </div><!-- ./header logo -->
                  {% assign hasMobileMenu = false %}
                  {% for block in section.blocks %}
                    {% if block.type == "mobile-menu" %}
                      {% assign hasMobileMenu = true %}
                    {% endif %}
                  {% endfor %}
                  {% if hasMobileMenu != blank %}
                    <div class="header__menu js-header-menu"><!-- header menu -->
                        <div class="header__close js-header-close">
                            <span class="material-icons">
                            close
                            </span>
                        </div>
                        <div class="custom-mobile-menu-wrapper">
                          <div class="custom-mobile-menu-images-wrapper">
                            {% assign mobileBannerCounter = 1 %}
                            {% for block in section.blocks %}
                              {% if block.type == "mobile-menu" %}
                                {% if block.settings.image != blank %}
                                  <div class="custom-mobile-menu-single-image {% if mobileBannerCounter == 1 %}active{% endif %} custom-mobile-{{mobileBannerCounter}}">
                                    <a href="{{ block.settings.banner_image_url }}">
                                      <img src="{{ block.settings.image | img_url: 'master' }}" loading="lazy" width="680" height="774" alt="" />
                                    </a>
                                  </div>
                                  {% assign mobileBannerCounter = mobileBannerCounter | plus: 1 %}
                                {% endif %}
                              {% endif %}
                            {% endfor %}
                            {% if mobileBannerCounter == 1 %}
                              <div class="cross-image-wrapper"></div>
                            {% endif %}
                          </div>
                          <div class="custom-mobile-menu-tabs-wrapper">
                            <div class="custom-mobile-menu-tab-title-listings">
                              <ul>
                                {% assign mobileTabTitleCounter = 1 %}
                                {% for block in section.blocks %}
                                  {% if block.type == "mobile-menu" %}
                                    {% if block.settings.title != blank %}
                                      <li class="{% if mobileTabTitleCounter == 1 %}active{% endif %}"><a href="custom-mobile-{{mobileTabTitleCounter}}">{{ block.settings.title }}</a></li>
                                      {% assign mobileTabTitleCounter = mobileTabTitleCounter | plus: 1 %}
                                    {% endif %}
                                  {% endif %}
                                {% endfor %}
                              <ul>
                            </div>
                            <div class="custom-mobile-menu-tab-content-listings">
                              {% assign mobileTabContentCounter = 1 %}
                              {% for block in section.blocks %}
                                {% if block.type == "mobile-menu" %}
                                  {% if block.settings.title != blank %}
                                    <div class="custom-mobile-menu-single-tab-content {% if mobileTabContentCounter == 1 %}active{% endif %} custom-mobile-{{mobileTabContentCounter}}">
                                      <div class="custom-mobile-menu-single-tab-menu">
                                        {% for i in (1..5) %}
                                          {% assign tab_menu_var = "tab_menu_" | append: i %}
                                          {% assign tab_menu_image_var = "tab_menu_" | append: i | append: "_image" %}
                                          {% assign tab_menu_title_var = "tab_menu_" | append: i | append: "_title" %}
                                          {% if block.settings[tab_menu_var] != blank and block.settings[tab_menu_image_var] != blank and block.settings[tab_menu_title_var] != blank %}
                                            <div class="custom-mobile-menu-single-tab-menu-details">
                                              <div class="custom-mobile-menu-single-tab-menu-details-title">
                                                <div class="custom-mobile-menu-single-tab-menu-details-title-image">
                                                  <img src="{{ block.settings[tab_menu_image_var] | img_url: '60x' }}" loading="lazy" width="60" height="60" alt="" />
                                                </div>
                                                <div class="custom-mobile-menu-single-tab-menu-details-title-name">
                                                  <p>{{ block.settings[tab_menu_title_var] }}</p>
                                                </div>
                                                <div class="custom-mobile-menu-single-tab-menu-details-title-icon"></div>
                                              </div>
                                              <div class="custom-mobile-menu-single-tab-menu-details-content">
                                                <ul>
                                                  {%- for mobileMenuLink in block.settings[tab_menu_var].links -%}
                                                    <li><a href="{{mobileMenuLink.url}}">{{mobileMenuLink.title | escape}}</a></li>
                                                  {%- endfor -%}
                                                </ul>
                                              </div>
                                            </div>
                                          {% endif %}
                                        {% endfor %}
                                      </div>
                                    </div>
                                  {% assign mobileTabContentCounter = mobileTabContentCounter | plus: 1 %}
                                  {% endif %}
                                {% endif %}
                              {% endfor %}
                            </div>
                          </div>
                        </div>
                    </div><!-- ./header menu -->
                  {% else %}
                    <div class="header__menu js-header-menu">
                      <!-- header menu -->
                      <div class="header__close js-header-close">
                        <span class="material-icons"> close </span>
                      </div>
                      <div class="custom-mobile-menu-wrapper">
                        <div class="custom-mobile-menu-images-wrapper">
                          <div class="custom-mobile-menu-single-image custom-mobile-1 active">
                            <a href="/collections/all">
                              {{ 'lifestyle-2' | placeholder_svg_tag: 'lazyload img-fluid svg__thumb' }}
                            </a>
                          </div>
                          <div class="custom-mobile-menu-single-image custom-mobile-2">
                            <a href="/collections/all">
